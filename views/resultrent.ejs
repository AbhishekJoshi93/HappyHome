<%- include("header") -%>
<%- include("nav") -%>
    <section class="dashboardsection">
        <div class="dashcontent">
            <div class="dashgrid">
                <div class="dashcontentcard">
                    <h4>For Lease <span><img src="/icon/deal.svg" alt="deal logo"></span></h4>
                    <div class="mainsalegrid">
                        <% leaseobj.forEach(function(username0){ %>
                        <% username0.leaseproperty.forEach(function(username){ %>
                            <% if(username.typeproperty == typeofp){ if(username.propertyarea == areapro){ if(username.propertycity == citypro){ if(username.propertystate == statepro){ if(username.propertybudget <= budgetprotxt){ if(username.propertyfurnishingtype == furnishedpro){ %>
                                <div class="salegrid">
    
                                <p class="grid0"><img src='uploads/<%=username.propertyimage%>' onerror="this.onerror=null; this.src='/image/default2.jfif'" alt="image upload"></p>
                                <p class="grid1"><span class="spanstyle">Name: </span><%=username.personname%></p>
                            <p><span class="spanstyle">Property Type: </span><%=username.typeproperty%></p>
                            <p><span class="spanstyle">Property Name: </span><%=username.propertyname%></p>
                            <p><span class="spanstyle">Property Address: </span><%=username.propertynumber%>,
                                <%=username.propertyfloor%> floor,
                                <%=username.propertyname%>,
                                <%=username.propertyarea%>,
                                <%=username.propertycity%>,
                                <%=username.propertystate%>,
                                <%=username.propertypincode%>.
                            </p>
                            <p><span class="spanstyle">Budget: </span><%=username.propertybudget%> Thousands.</p>
                            <p><span class="spanstyle">Rera Approved: </span><%=username.propertyreraapproved%></p>
                            <p><span class="spanstyle">Security Deposit: </span><%=username.propertysecuritydeposit%></p>
                            <p><span class="spanstyle">Preference: </span><%=username.propertypreference%></p>
                            <p><span class="spanstyle">Property Age: </span><%=username.propertyage%></p> 
                            <p><span class="spanstyle">Negotaiation: </span><%=username.propertynegotiation%></p> 
                            <p><span class="spanstyle">Number Of Rooms: </span><%=username.propertynumberrooms%></p> 
                            <p><span class="spanstyle">Number Of Bathrooms: </span><%=username.propertynumberbathrooms%></p> 
                            <p><span class="spanstyle">Property Sq Area: </span><%=username.propertysquarearea%></p>
                            <p><span class="spanstyle">Property Facing: </span><%=username.propertyfacing%></p>
                            <p class="customgrid"><span class="spanstyle">Near By:</span>
                                <% username.propertynearby.forEach(function(element) { %>
                                   <p>
                                    <% if(element === "hospital") { %>
                                        <img style="width: 25%;" src="/icon/hospital.svg" alt="icons">
                                    <% } %>
                                    <% if(element === "school") { %>
                                        <img style="width: 25%;" src="/icon/school.svg" alt="icons">
                                    <% } %>
                                    <% if(element === "shoppingmall") { %>
                                        <img style="width: 25%;" src="/icon/shoppingmall.svg" alt="icons">
                                    <% } %>
                                    <% if(element === "carparking") { %>
                                        <img style="width: 25%;" src="/icon/carparking.svg" alt="icons">
                                    <% } %>
                                    <% if(element === "atm") { %>
                                        <img style="width: 25%;" src="/icon/atm.svg" alt="icons">
                                    <% } %>
                                   </p>
                                <% }) %>
                            </p>
                            <p class="customgrid"><span class="spanstyle">Amenities:</span>
                                <% username.propertyamenities.forEach(function(element) { %>
                                    <p>
                                        <% if(element === "lift") { %>
                                            <img style="width: 25%;" src="/icon/lift.svg" alt="icons">
                                        <% } %>
                                        <% if(element === "garden") { %>
                                            <img style="width: 25%;" src="/icon/garden.svg" alt="icons">
                                        <% } %>
                                        <% if(element === "security") { %>
                                            <img style="width: 25%;" src="/icon/security.svg" alt="icons">
                                        <% } %>
                                        <% if(element === "clubhouse") { %>
                                            <img style="width: 25%;" src="/icon/clubhouse.svg" alt="icons">
                                        <% } %>
                                        <% if(element === "gymnasium") { %>
                                            <img style="width: 25%;" src="/icon/gymnasium.svg" alt="icons">
                                        <% } %>
                                    </p>
                                <% }) %>
                            </p>
                            <p class="customgrid"><span class="spanstyle">Furnishing:</span>
                                <% username.propertyfurnishing.forEach(function(element) { %>
                                    <p>
                                        <% if(element === "wardrobe") { %>
                                            <img style="width: 25%;" src="/icon/wardrobe.svg" alt="icons">
                                        <% } %>
                                        <% if(element === "gasconnection") { %>
                                            <img style="width: 25%;" src="/icon/gasconnection.svg" alt="icons">
                                        <% } %>
                                        <% if(element === "dinning") { %>
                                            <img style="width: 25%;" src="/icon/dinning.svg" alt="icons">
                                        <% } %>
                                        <% if(element === "refrigerator") { %>
                                            <img style="width: 25%;" src="/icon/refrigerator.svg" alt="icons">
                                        <% } %>
                                        <% if(element === "bed") { %>
                                            <img style="width: 25%;" src="/icon/bed.svg" alt="icons">
                                        <% } %>
                                    </p>
                                <% }) %>
                            </p>

                                <div class="formsection grid10">
                                    <div class="element">
                                        <form action="/interest" method="post" class="formsection">
                                            <label for="roomid">Unique Room Id: </label>
                                            <input type="text" name="roomid" class="input" placeholder="eg. Top Secret" style="width: 25%;">
                                            <br>
                                            <pre><b>* Enter unique roomid for communication and this will be delieverd to property owner<br> with default interesed message for further communication.<br>Use room id in chat section with property owner</b></pre>
                                            <div class="elementbtn">
                                                <button type="submit" style="width: 25%;">Interested</button>
                                            </div>
                                            <input type="text" name="refid" class="inputvis" value=<%=username.personnumber%>>
                                        </form>
                                    </div>
                                </div>
                                <% }}}}}} %>
                            </div>
                            <div class="justbreak">
                            </div>
                        <% }) %>
                            <br>
                        <% }) %>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <script>
        $("document").ready( function () {
            alertify.alert('Searched For', 'Address:<%=areapro%>,<%=citypro%>,<%=statepro%> Budget:â‚¹<%=budgetprotxt%>', function(){ alertify.error('OK') });
        });
    </script>
<%- include("bar") -%>
<%- include("footer") -%>