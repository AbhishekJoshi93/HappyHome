        <h1>Leave Feedback For Our Developer</h1>
        <div class="formsection">
            <div class="element">
                <form action="/rating" method="POST">
                    <h3>Feedback <span><img src="/icon/dealer.svg" alt="dealer logo"></h3>
                        <input type="range" name="ratingpro" id="eleduration" min="1" max="5">
                        <span id="eledurationout"></span>                        
                        <br>
                        <pre>* Feedback scale is on 1 to 5</pre>
                        <div class="elementbtn" style="margin: -3% 0 -3% 0;">
                            <button type="submit" class="btn">Submit</button>
                        </div>
                </form>
            </div>
        </div>
    </section>

<%- include("bar") -%>

    <script>
        $('.count').each(function () {
        $(this).prop('Counter',0).animate({
            Counter: $(this).text()
        }, {
            duration: 4000,
            easing: 'swing',
            step: function (now) {
                $(this).text(Math.ceil(now));
            }
        });
    });
    </script>
    <script>
        var slider = document.getElementById("eleduration");
    var output = document.getElementById("eledurationout");
    output.innerHTML = slider.value;
    slider.oninput = function() {
      output.innerHTML = this.value;
    }
    </script>

